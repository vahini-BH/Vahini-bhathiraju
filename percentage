#include<stdio.h>
void main()
{
int percentage;
printf("enter percentage");
scanf("%d",&percentage);
if(percentage>=90)
{
printf("distinction");
printf("%d",percentage);
}
else if (percentage<=89&&percentage>=70)
printf("first class");
else if(percentage<=45&&percentage>=69)
printf("second class");
else 
{
printf("fail");
}
//return;
}

output:
enter percentage
75
first class
